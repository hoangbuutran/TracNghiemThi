@using Model.EF


@{
    ViewBag.Title = "LichSuThi";
    Layout = "~/Views/Shared/_LayoutSecond.cshtml";
    var dsLichSu = (List<BAILAM>)ViewBag.ds;
}

<h2>Xem lại lịch sử thi</h2>


<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                <strong>Danh Sách Lịch sử thi</strong> 
            </div>
            <!-- /.panel-heading -->
            <div class="panel-body">
                <div class="table-responsive">
                    <table class="table table-striped table-bordered table-hover">
                        <thead>
                        <tr>
                            <th>
                                <strong>Tên đề thi</strong>
                            </th>
                                
                            <th>
                                <strong>Ngày thi</strong>
                            </th>
                            <th>
                                <strong>Điểm số</strong>
                            </th>
                        </tr>
                        </thead>
                        <tbody>
                        @foreach (var item in dsLichSu)
                        {
                            <tr>
                                <td>
                                    @item.DETHI.TENDETHI
                                </td>
                                    
                                <td>
                                    @item.NGAY
                                </td>
                                <td>
                                    @item.DIEM/@item.DETHI.SOCAU.SOCAU1
                                </td>
                            </tr>
                        }
                        </tbody>
                    </table>
                </div>
                <!-- /.table-responsive -->
            </div>
            <!-- /.panel-body -->
        </div>
        <!-- /.panel -->
    </div>

</div>